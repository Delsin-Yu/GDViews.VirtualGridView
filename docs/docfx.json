{
	"metadata": [
		{
			"src": [
				{
					"src": ".././GDViews.VirtualGridView",
					"files": [
						"**/*.csproj"
					]
				}
			],
			"dest": "api",
			"outputFormat": "apiPage",
			"filter": "filterConfig.yml"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**",
					"index.md",
					"toc.yml"
				]
			}
		],
		"output": "_site",
		"template": [
			"default",
			"modern"
		],
		"globalMetadata": {
			"_appName": "GDViews.VirtualGridView",
			"_appTitle": "GDViews.VirtualGridView",
			"_enableSearch": true,
			"pdf": false
		}
	}
}